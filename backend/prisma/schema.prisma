generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("VITE_POSTGRES_URL")
}

model user {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String
  role_id Int
  role role @relation(fields: [role_id], references: [id])
}

model role{
  id Int @id @default(autoincrement())
  name String @unique
  user user[]
}